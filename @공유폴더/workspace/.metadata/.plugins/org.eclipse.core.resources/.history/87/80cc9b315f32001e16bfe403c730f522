package chapter2;

import java.util.Scanner;

public class practice4 {
	public static void main(String [] args) {
		//1번
		int num1=10;
		int num2=2;
		char operator = '+';
		
		if(operator == '+') {
			System.out.println(num1 + num2);
		}else if(operator == '-'){
			System.out.println(num1 - num2);
		}else if(operator == '*'){
			System.out.println(num1 * num2);
		}else if(operator == '/'){
			System.out.println(num1 / num2);
		}
		
		switch(operator){
			case '+' :
				System.out.println(num1 + num2);
				break;
			case '-' :
				System.out.println(num1 - num2);
				break;
			case '*' :
				System.out.println(num1 * num2);
				break;
			case '/' :
				System.out.println(num1 / num2);
				break;
		}
		
		//2번
		System.out.println("2번");
		for(int i=2; i<=9; i++) {
			for(int j=1; j<=9; j++) {
				System.out.println(i+"x"+j+"="+i*j);
			}
			System.out.println();
		}
			
	}
}
