package chapter2;

import java.util.Scanner;

public class 가위바위보 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner s = new Scanner(System.in); 
		String str = s.nextLine();
		String computer = "가위";
		if(str.equals("가위")) {
			if(computer.equals("가위")) {
				System.out.println("비겼습니다.");
			}
		}
	}

}
