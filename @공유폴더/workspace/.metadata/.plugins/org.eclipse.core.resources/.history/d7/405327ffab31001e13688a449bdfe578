package chapter2;

public class test01 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num1 = 4;
		int num2 = 10;
		int result = num1 & num2;
		System.out.println(result);
		result = num1 | num2;
		System.out.println(result);
		result = num1 ^ num2;
		
		System.out.println(result);
		
		
	}

}
