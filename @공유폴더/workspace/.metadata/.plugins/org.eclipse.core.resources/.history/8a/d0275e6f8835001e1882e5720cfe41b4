package array;

public class BookArray2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
//		ctrl + shift + f
		Book[] library = new Book[5];
		
		library[0] = new Book("","");
		library[0] = new Book("","");
		library[0] = new Book("","");
		library[0] = new Book("","");
		library[0] = new Book("","");
		
		for(int i=0; i<library.length; i++) {
			System.out.println(library[i]);
		}
	}

}
