package practice2;

import java.util.Scanner;

public class Q1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner s = new Scanner(System.in);
		System.out.println("영문소문자 하나를 입력해주세요.");
		String str = s.next();
		char c = str.charAt(0);
		for(int i=96; i<96-c; i++) {
			for(int j=0; j<10; j++) {
				System.out.print(c);
			}
			System.out.println();
		}
	}

}
